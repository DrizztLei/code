        </receiver>
        <receiver android:name=".ServiceWork.DotService$DotServiceReciever">

        </receiver>

        <service android:name=".ServiceWork.DotService">

        </service>
        <receiver android:name="sdu.embedded.SonicCollector.CollectorService$CollectorServerReceiver">

        </receiver>
    </application>

</manifest>
