
import java.io.IOException;
import libsvm.*;

public class comMain {
	private static comMain com;
	int count = 256;

	public static void main(String[] args) throws IOException {
		comMain test = comMain.getInstead();
		test.calculator(new double[2]);
	}

	public comMain() {

	}

	public static comMain getInstead() {
		if (com == null) {
			com = new comMain();
		}
		return com;
	}

	public int calculator(double[] info) throws IOException {
		String[] parg = { "/home/elvis/final_svm", // 这个是存放测试数据
				"/home/elvis/final_svm.model", // 调用的是训练以后的模型
				"/home/elvis/out" }; // 生成的结果的文件的路径
		System.out.println("........SVM运行开始..........");
		//svm_predict p = new svm_predict();
		//svm_predict.main(parg); // 调用
		svm_node node[] = new svm_node[1];
		node[0] = new svm_node();
		node[0].index = 0;
		node[0].value = 1;
		
		return 0;
	}

}